            <parent link="rr_steering_wheel_link"/>
            <child link="rr_wheel_link"/>
            <axis xyz="0 1 0" rpy="0 0 0" />
        </joint>

    <!-- Chair Structure -->
    <!-- Chair Seat -->
    <link name="chair_seat">
        <inertial>
            <origin xyz="0.0 0.0 0.0"/>
            <mass value="5"/>
            <inertia ixx="0.2" ixy="0.0" ixz="0.0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.4 0.35 0.05" />
            </geometry>
            <material name="chair_black" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.4 0.35 0.05" />
            </geometry>
        </collision>
    </link>

    <!-- Chair Back -->
    <link name="chair_back">
        <inertial>
            <origin xyz="0.0 0.0 0.0"/>
            <mass value="3"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.4 0.05 0.4" />
            </geometry>
            <material name="chair_black" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.4 0.05 0.4" />
            </geometry>
        </collision>
    </link>

    <!-- Chair Legs -->
