<?xml version="1.0"?>

<robot name="ranger_mini_patient_chair" 
    xmlns:xacro="http://ros.org/wiki/xacro">

    <!-- Material definitions -->
    <material name="black">
        <color rgba="0.1 0.1 0.1 1" />
    </material>
    <material name="white">
        <color rgba="1 1 1 1" />
    </material>
    <material name="dark_grey">
        <color rgba="0.3 0.3 0.3 1" />
    </material>
    <material name="metallic_grey">
        <color rgba="0.7 0.7 0.7 1" />
    </material>
    <material name="chair_black">
        <color rgba="0.05 0.05 0.05 1" />
    </material>
    <material name="chair_metal">
        <color rgba="0.8 0.8 0.8 1" />
    </material>

    <!-- Base link -->
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0"/>
            <mass value="10"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="1.57 0 3.14 " />
            <geometry>
                <mesh filename="package://ranger_mini_v2/meshes/ranger_base.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.1" rpy="0 0 0" />
            <geometry>
                <box size="0.5 0.35 0.2"/>
            </geometry>
        </collision>
 
    </link>

   <link name="fl_steering_wheel_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0"/>
            <mass value="1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="-0.23 -0.194 0.1" rpy="-3.14 0 3.14 " />
            <geometry>
                    <mesh filename="package://ranger_mini_v2/meshes/fl.stl" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.008" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.001" length="0.001"/>
            </geometry>
        </collision>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.001" length="0.001"/>
            </geometry>
        </collision>
    </link>
    <joint name="fl_steering_wheel" type="revolute">
        <origin xyz="0.23 0.206 -0.1" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="fl_steering_wheel_link"/>
        <axis xyz="0 0 1" rpy="0 0 0" />
        <limit effort="5" velocity="6.28"  lower="-2.1" upper="2.1" />
    </joint>

        <link name="fl_wheel_link">
            <inertial>
                <mass value="8" />
                <origin xyz="0 0 0" />
                <inertia ixx="0.1171" ixy="0" ixz="0" iyy="0.1361" iyz="0" izz="0.1171" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="1.57 0 0" />
                <geometry>
                    <cylinder length="0.01" radius="0.01" />
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="1.57 0 0" />
                <geometry>
                    <cylinder length="0.08" radius="0.08" />
                </geometry>
                     <contact>
                         <ode>
                        <min_depth>0.001</min_depth>
                         </ode>
                    </contact>
            </collision>
        </link>

        <joint name="fl_wheel" type="continuous">
            <origin xyz="0.03 0.01 -0.12" rpy="0 0 0" />
            <parent link="fl_steering_wheel_link"/>
            <child link="fl_wheel_link"/>
            <axis xyz="0 1 0" rpy="0 0 0" />
        </joint>


   <link name="fr_steering_wheel_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin xyz="-0.23 0.194 0.1" rpy="-3.14 0 3.14 " />
            <geometry>
                <mesh filename="package://ranger_mini_v2/meshes/fr.stl" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.008" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.001" length="0.001"/>
            </geometry>
        </collision>
    </link>
    <joint name="fr_steering_wheel" type="revolute">
        <origin xyz="0.23 -0.206 -0.1" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="fr_steering_wheel_link"/>
        <axis xyz="0 0 1" rpy="0 0 0" />
        <limit effort="5" velocity="6.28"  lower="-2.1" upper="2.1" />
    </joint>

        <link name="fr_wheel_link">
            <inertial>
                <mass value="8" />
                <origin xyz="0 0 0" />
                <inertia ixx="0.1171" ixy="0" ixz="0" iyy="0.1361" iyz="0" izz="0.1171" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="1.57 0 0" />
                <geometry>
                    <cylinder length="0.01" radius="0.01" />
