    <!-- Chair Joints -->
    <joint name="chair_seat_joint" type="fixed">
        <origin xyz="0 0 0.15" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="chair_seat"/>
    </joint>

    <joint name="chair_back_joint" type="fixed">
        <origin xyz="0.15 0 0.35" rpy="0 0 0" />
        <parent link="chair_seat"/>
        <child link="chair_back"/>
    </joint>

    <joint name="chair_leg1_joint" type="fixed">
        <origin xyz="0.15 0.15 0.15" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="chair_leg1"/>
    </joint>

    <joint name="chair_leg2_joint" type="fixed">
        <origin xyz="0.15 -0.15 0.15" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="chair_leg2"/>
    </joint>

    <joint name="chair_leg3_joint" type="fixed">
        <origin xyz="-0.15 0.15 0.15" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="chair_leg3"/>
    </joint>

    <joint name="chair_leg4_joint" type="fixed">
        <origin xyz="-0.15 -0.15 0.15" rpy="0 0 0" />
